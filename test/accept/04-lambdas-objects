var animal = {
    legs: 2,
    heads: 5,
    eyes: 3,
    name: "Bob",
    actions: {
        greet: function() {
            return "Hello, my name is " + this.name;
        }
        eat: (food) => {
            return "I am eating " + food;
        }
    } 
}

assertTrue (animal.legs == 2);
animal.actions = 4; // Not implemented yet :(
animal.actions.greet();
animal.actions.eat("food");