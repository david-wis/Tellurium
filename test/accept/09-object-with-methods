var animal = {
    legs: 4,
    heads: 5,
    eyes: 3,
    name: "Bob",
    actions: {
        greet: function() {
            return "Hello, my name is " + this.name;
        },
        eat: (food)=>{
            return "I am eating " + food;
        },
        walk: (distance   )             =>
        {
            return "I am walking " + distance ;
        }
    } 
};

assertTrue animal.legs == ((1+1)*2);
animal.legs = 3; 
animal.actions.greet();
animal.actions.eat("food");